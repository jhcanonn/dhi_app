  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write --ignore-unknown",
    "format:check": "prettier . --check",
    "prepare": "husky install",
    "restart": "pm2 restart front-app",
    "clean-temp": "rm -rf .next-temp node_modules",
    "clean": "rm -rf .next",
    "move-build": "mv .next-temp .next",
    "production-build": "npm run clean-temp && npm install && npm run build && npm run clean && npm run move-build"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/x-date-pickers": "^6.18.1",
    "@next/bundle-analyzer": "14.0.3",
    "@react-hook/window-size": "^3.1.1",
    "@tanstack/react-query": "^5.8.4",
    "autoprefixer": "10.4.16",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "graphql": "^16.8.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "moment": "^2.29.4",
    "next": "^14.0.3",
    "next-auth": "^4.24.5",
    "nprogress": "^0.2.0",
    "pdfmake": "^0.2.8",
    "postcss": "^8.4.31",
