    "primeicons": "^6.0.1",
    "primereact": "^9.6.3",
    "react": "18.2.0",
    "react-content-loader": "^6.2.1",
    "react-cookie": "^6.1.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.47.0",
    "sass": "^1.69.5",
    "sharp": "^0.32.6",
    "tailwindcss": "3.3.5",
    "url-template": "^3.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.0.3",
    "@tanstack/eslint-plugin-query": "^5.8.4",
    "@types/jspdf": "^2.0.0",
    "@types/node": "20.9.2",
    "@types/nprogress": "^0.2.3",
    "@types/pdfmake": "^0.2.8",
    "@types/react": "18.2.37",
    "@types/react-cookies": "^0.1.3",
    "@types/react-dom": "18.2.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*": "prettier --write",
    "*.ts": "eslint --fix",
    "*.tsx": "eslint --fix"
  }
}
